<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#1a000000"/>
            <corners android:topLeftRadius="@dimen/_8dp" android:bottomLeftRadius="@dimen/_8dp"/>
        </shape>
    </item>
    <item android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="@dimen/_1dp">
        <shape>
            <corners android:topLeftRadius="@dimen/_8dp" android:bottomLeftRadius="@dimen/_8dp"/>
            <solid android:color="@color/colorBackgroundLayout"/>
        </shape>
    </item>
</layer-list>
